---
layout: layouts/home.njk
eleventyNavigation:
  key: Featured
  order: 2
---
<div>
<div class="">All ({{matterport.length}})</div>
{% if matterport | selectattr("featured") | length > 0 %}
<div class="">
 {% if not '/featured' in page.url %} <a href="/featured">{% endif %}
  Featured ({{ matterport | selectattr("featured") | length }})
  {% if not '/featured' in page.url %} </a>{% endif %}
  </div>
{% endif %}
</div>

{% include 'paginate.njk'%}
<div class="row resultcontainer">
{% for result in matterport %}
{% if (result.featured === true) and (result.visible === true) %}
<div class="result col-xs-12 col-sm-8 col-md-6 col-lg-2">
<a href="https://my.matterport.com/show/?m={{result.id}}&help=0&hl=0&qs=1&brand=0&dh=1&gt=0&hr=0&mls=2&mt=0&pin=0&play=1" target="_blank" rel="noopener">
<img src="https://my.matterport.com/api/v2/player/models/{{result.id}}/thumb/" width="100%" height="auto">
</a>
</div>
{% endif %}
{% endfor %}
</div> 

{% include 'paginate.njk'%}
